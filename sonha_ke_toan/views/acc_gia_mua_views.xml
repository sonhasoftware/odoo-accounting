<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_acc_gia_mua_tree" model="ir.ui.view">
        <field name="name">acc.gia.mua.tree</field>
        <field name="model">acc.gia.mua</field>
        <field name="arch" type="xml">
            <tree string="Danh mục giá mua" editable="bottom">
                <field name="NGAY_HL" class="check_cap" optional="show"/>
                <field name="HANG_HOA" class="check_ma" optional="show"/>
                <field name="KHACH_HANG" class="check_ten" optional="show"/>
                <field name="DON_GIA" class="check_kho" optional="show"/>
                <field name="DVCS" class="check_dv" optional="show"/>
                <field name="ACTIVE" class="check_at" optional="show"/>
                <field name="create_uid" string="Người tạo" optional="hide" class="check_cr"/>
                <field name="write_uid" string="Người sửa" optional="hide" class="check_wt"/>
                <field name="create_date" string="Ngày tạo" optional="hide" class="check_cd"/>
                <field name="write_date" string="Ngày sửa" optional="hide" class="check_wd"/>
            </tree>
        </field>
    </record>

    <record id="acc_gia_mua_action" model="ir.actions.act_window">
        <field name="name">Danh mục giá mua</field>
        <field name="res_model">acc.gia.mua</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>