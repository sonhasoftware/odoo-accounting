<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_acc_bao_cao_tree" model="ir.ui.view">
        <field name="name">acc.bao.cao.tree</field>
        <field name="model">acc.bao.cao</field>
        <field name="arch" type="xml">
            <tree string="Danh mục báo cáo" editable="bottom">
                <field name="MA" optional="show"/>
                <field name="TEN" optional="show"/>
                <field name="BO_PHAN" optional="show"/>
                <field name="KHOAN_MUC" optional="show"/>
                <field name="HANG_HOA" optional="show"/>
                <field name="KHO" optional="show"/>
                <field name="TSCD" optional="show"/>
                <field name="VVIEC" optional="show"/>
                <field name="KHACH_HANG" optional="show"/>
                <field name="FN_BAO_CAO" optional="show"/>
                <field name="TU_TK" optional="show"/>
                <field name="DK_DAU" optional="show"/>
                <field name="TINH_SAU" optional="show"/>
                <field name="DK_SAU" optional="show"/>
                <field name="DK_GOI_BC" optional="show"/>
                <field name="FN_GOI_BC" optional="show"/>
                <field name="field_ids" optional="show" widget="many2many_tags"/>
            </tree>
        </field>
    </record>

    <record id="acc_bao_cao_action" model="ir.actions.act_window">
        <field name="name">Danh mục báo cáo</field>
        <field name="res_model">acc.bao.cao</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>