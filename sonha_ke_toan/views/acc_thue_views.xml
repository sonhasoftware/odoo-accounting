<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_acc_thue_tree" model="ir.ui.view">
        <field name="name">acc.thue.tree</field>
        <field name="model">acc.thue</field>
        <field name="arch" type="xml">
            <tree string="Danh mục thuế" editable="bottom">
                <field name="TEN"/>
                <field name="PT_THUE"/>
            </tree>
        </field>
    </record>

    <record id="acc_thue_action" model="ir.actions.act_window">
        <field name="name">Danh mục thuế</field>
        <field name="res_model">acc.thue</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>