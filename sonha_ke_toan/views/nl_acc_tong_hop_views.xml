<odoo>

    <record id="view_nl_acc_tong_hop_tree" model="ir.ui.view">
        <field name="name">nl.acc.tong.hop.tree</field>
        <field name="model">nl.acc.tong.hop</field>
        <field name="arch" type="xml">
            <tree string="Tổng hợp" create="false" editable="bottom" edit="true" js_class="button_click_f9">
                <field name="NGAY_CT" optional="show"/>
                <field name="CHUNG_TU" optional="show"/>
                <field name="SO_HD" optional="show"/>
                <field name="SERI_HD" optional="show"/>
                <field name="NGAY_HD" optional="show"/>
                <field name="PS_NO1" optional="show"/>
                <field name="TIEN_NTE" optional="show"/>
                <field name="MA_TK0_ID" optional="show"/>
                <field name="MA_TK1_ID" optional="show"/>
                <field name="TIEN_TE" optional="show"/>
                <field name="TY_GIA" optional="show"/>
                <field name="HANG_HOA" optional="show"/>
                <field name="SO_LUONG" optional="show"/>
                <field name="DON_GIA" optional="show"/>
                <field name="KHACH_HANG" optional="show"/>
                <field name="KHACH_HANGC" optional="show"/>
                <field name="TSCD" optional="show"/>
                <field name="KHOAN_MUC" optional="show"/>
                <field name="BO_PHAN" optional="show"/>
                <field name="VVIEC" optional="show"/>
                <field name="SAN_PHAM" optional="show"/>
                <field name="KHO" optional="show"/>
                <field name="KHOC" optional="show"/>
                <field name="MS_THUE" optional="show"/>
                <field name="DC_THUE" optional="show"/>
                <field name="KH_THUE" optional="show"/>
                <field name="MENU_ID" optional="show"/>
                <field name="create_uid" string="User nhập" optional="show"/>
                <field name="write_uid" string="User sửa" optional="show"/>
                <field name="PT_THUE" optional="show"/>
                <field name="THUE_NK" optional="show"/>
            </tree>
        </field>
    </record>


    <record id="action_nl_acc_tong_hop" model="ir.actions.act_window">
        <field name="name">Tổng hợp</field>
        <field name="res_model">nl.acc.tong.hop</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="view_nl_acc_tong_hop_list_js" model="ir.ui.view">
        <field name="name">nl.acc.tong.hop.list.js</field>
        <field name="model">nl.acc.tong.hop</field>
        <field name="inherit_id" ref="sonha_ke_toan.view_nl_acc_tong_hop_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="js_class">button_in_tree</attribute>
            </xpath>
        </field>
    </record>

</odoo>