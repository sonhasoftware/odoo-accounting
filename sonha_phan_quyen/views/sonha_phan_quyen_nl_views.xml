<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_sonha_phan_quyen_nl_tree" model="ir.ui.view">
        <field name="name">sonha.phan.quyen.nl.tree</field>
        <field name="model">sonha.phan.quyen.nl</field>
        <field name="arch" type="xml">
            <tree string="Phân quyền" editable="bottom">
                <field name="MENU"/>
                <field name="BO_PHAN"/>
                <field name="KHO"/>
                <field name="KHOAN_MUC"/>
                <field name="VVIEC"/>
                <field name="LOAI_NX"/>
                <field name="LOAI_DL"/>
                <field name="CHI_NHANH"/>
                <field name="MA_TK0_ID" domain="[(1, '=', 1)]"/>
                <field name="MA_TK1_ID" domain="[(1, '=', 1)]"/>
                <field name="TIEN_TE"/>
                <field name="KT_SL"/>
                <field name="GIA_MUA"/>
                <field name="GIA_BAN"/>
                <field name="MOT_VE"/>
                <field name="KHONG_TINH_GV"/>
                <field name="CT_KHONG_TD"/>
                <field name="PT_VAT"/>
                <field name="CHUNG_TU"/>
                <field name="DVCS"/>
                <field name="ACTIVE"/>
            </tree>
        </field>
    </record>

    <record id="sonha_phan_quyen_nl_action" model="ir.actions.act_window">
        <field name="name">Phân Quyền Nhập Liệu</field>
        <field name="res_model">sonha.phan.quyen.nl</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="menu_sonha_phan_quyen_nl"
              name="Phân quyền nhập liệu"
              parent="menu_sonha_phan_quyen_user"
              action="sonha_phan_quyen_nl_action"
              sequence="3"/>
</odoo>